<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/primaryColor"
    tools:context=".HomeFragment">

    <EditText
        android:id="@+id/searchEditText"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_margin="15dp"
        android:autofillHints=""
        android:paddingStart="15dp"
        android:paddingEnd="16dp"
        android:paddingTop="12dp"
        android:paddingBottom="12dp"
        android:textColor="@android:color/white"
        android:hint="@string/search"
        android:background="@drawable/edit_text_round"
        android:textColorHint="@color/white"
        android:textSize="13sp"
        android:inputType="text"
        android:importantForAccessibility="yes"
        android:contentDescription="@string/search_field"
        tools:ignore="ContentDescription" />


    <Space
        android:layout_width="match_parent"
        android:layout_height="5dp" />


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/carouselRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="120dp"

        android:orientation="horizontal"
        android:padding="8dp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="15dp"
       >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_alignParentStart="true"
            android:gravity="center_vertical"
            tools:ignore="RelativeOverlap,UseCompoundDrawables">

            <ImageView
                android:id="@+id/iconImageView"
                android:layout_width="23dp"
                android:layout_height="23dp"
                android:layout_marginStart="5dp"
                android:contentDescription="@string/iconName"
                android:src="@drawable/ic_start"

                card_view:tint="@color/gray" />

            <TextView
                android:id="@+id/numberTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="text one"
                android:textColor="@color/white"
                android:textSize="13sp" />
        </LinearLayout>


        <ImageView
            android:id="@+id/iconImageView2"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:contentDescription="@string/iconName"
            android:src="@drawable/ic_arrow"
            android:layout_alignParentEnd="true"
            android:layout_centerInParent="true"
            card_view:tint="@color/gray" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="15dp"
        android:layout_marginTop="12dp"
        >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_alignParentStart="true"
            android:gravity="center_vertical"
            tools:ignore="RelativeOverlap,UseCompoundDrawables">

            <ImageView
                android:id="@+id/iconImageView23"
                android:layout_width="23dp"
                android:layout_height="23dp"
                android:layout_marginStart="5dp"
                android:contentDescription="@string/iconName"
                android:src="@drawable/ic_start"


                card_view:tint="@color/gray" />

            <TextView
                android:id="@+id/numberTextView33"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="text one"
                android:textColor="@color/white"
                android:textSize="13sp" />
        </LinearLayout>


        <ImageView
            android:id="@+id/iconImageView233"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:contentDescription="@string/iconName"
            android:src="@drawable/ic_arrow"
            android:layout_alignParentEnd="true"
            android:layout_centerInParent="true"
            card_view:tint="@color/gray" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="15dp"
        android:layout_marginTop="12dp"
        >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:layout_alignParentStart="true"
            tools:ignore="RelativeOverlap,UseCompoundDrawables">

            <ImageView
                android:id="@+id/iconImageView23133"
                android:layout_width="23dp"
                android:layout_height="23dp"
                android:layout_marginStart="5dp"
                android:contentDescription="@string/iconName"
                android:src="@drawable/ic_start"


                card_view:tint="@color/gray" />

            <TextView
                android:id="@+id/numberTextView3333"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="text one"
                android:textColor="@color/white"
                android:textSize="13sp" />
        </LinearLayout>


        <ImageView
            android:id="@+id/iconImageView23333"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:contentDescription="@string/iconName"
            android:src="@drawable/ic_arrow"
            android:layout_alignParentEnd="true"
            android:layout_centerInParent="true"
            card_view:tint="@color/gray" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="15dp"
        android:layout_marginTop="10dp"
        >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:layout_alignParentStart="true"
            tools:ignore="RelativeOverlap,UseCompoundDrawables">

            <ImageView
                android:id="@+id/iconImageView2333"
                android:layout_width="23dp"
                android:layout_height="23dp"
                android:layout_marginStart="5dp"
                android:contentDescription="@string/iconName"
                android:src="@drawable/ic_start"


                card_view:tint="@color/gray" />

            <TextView
                android:id="@+id/numberTextView33333"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="text one"
                android:textColor="@color/white"
                android:textSize="13sp" />
        </LinearLayout>


        <ImageView
            android:id="@+id/iconImageView233333"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:contentDescription="@string/iconName"
            android:src="@drawable/ic_arrow"
            android:layout_alignParentEnd="true"
            android:layout_centerInParent="true"
            card_view:tint="@color/gray" />
    </RelativeLayout>
</LinearLayout>
